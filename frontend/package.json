    "react-router-dom": "^6.17.0",
    "sirv": "^2.0.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@vitejs/plugin-react-swc": "^3.3.2",
